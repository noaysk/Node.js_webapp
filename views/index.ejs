<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>LIST APP</title>
    <link rel="stylesheet" href="/css/style.css" />
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <a href="/" class="header-logo"  >LIST APP</a>
    </header>
    <div class="container">
      <div class="container-header">
        <h1>Buying List</h1>
        <a href="/new" class="new-button">+ new</a>
      </div>
      <div class="index-table-wrapper">
        <div class="table-head">
          <span class="id-column">Number</span>
          <span>List</span>
        </div>
       
        <ul class="table-body">
          <% items.forEach((item) => { %>
          <li>
            <span class="id-column"><%= item.number %></span>
            <span class="name-column"><%= item.name %></span>
          </li>
          <% }); %>
        </ul>
      </div>
    </div>
  </body>
</html>
